{
  "name": "bdit_flashcrow",
  "version": "0.2.0",
  "private": true,
  "description": "An integrated data platform for City of Toronto transportation data",
  "homepage": "https://move.intra.dev-toronto.ca",
  "scripts": {
    "backend": "node -r esm -r module-alias/register web/server.js",
    "backend:inspect": "node -r esm -r module-alias/register --inspect=0.0.0.0:9281 web/server.js",
    "backend:inspect-brk": "node -r esm -r module-alias/register --inspect-brk=0.0.0.0:9281 web/server.js",
    "backend:test-api": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass API_TEST_HEADLESS=1 node -r esm -r module-alias/register web/server.js",
    "ci:jest-coverage": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass jest --coverage",
    "ci:npm-audit": "npm audit",
    "ci:npm-outdated": "npm outdated || true",
    "ci:test-unit": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"unit/**\" jest",
    "docs:js": "documentation serve --babel babel.config.js --favicon public/favicon.ico --port 9080 --sort-order alpha --watch web/server.js lib/**",
    "frontend": "vue-cli-service serve",
    "frontend:build": "vue-cli-service build",
    "pre-commit:lint-staged": "lint-staged",
    "pre-commit:test-unit-staged": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"unit/**\" jest --changedSince=master",
    "reporter": "node -r esm -r module-alias/register reporter/reporter.js",
    "reporter:inspect": "node -r esm -r module-alias/register --inspect=0.0.0.0:9282 reporter/reporter.js",
    "reporter:inspect-brk": "node -r esm -r module-alias/register --inspect-brk=0.0.0.0:9282 reporter/reporter.js",
    "reporter:test-api": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass node -r esm -r module-alias/register reporter/reporter.js",
    "test:test-api": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"api/**\" jest --runInBand",
    "test:test-db": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"db/**\" jest --runInBand",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build"
  },
  "dependencies": {
    "@hapi/boom": "^8.0.1",
    "@hapi/cookie": "^10.1.2",
    "@hapi/crumb": "^7.3.2",
    "@hapi/good": "^8.2.4",
    "@hapi/good-console": "^8.1.2",
    "@hapi/good-squeeze": "^5.2.1",
    "@hapi/hapi": "^18.4.0",
    "@hapi/joi": "^15.1.1",
    "@hapi/scooter": "^5.1.1",
    "@sendgrid/mail": "^6.4.0",
    "aws-sdk": "^2.553.0",
    "blankie": "^4.1.1",
    "csv-generate": "^3.2.3",
    "csv-stringify": "^5.3.3",
    "d3-array": "^2.3.3",
    "d3-axis": "^1.0.12",
    "d3-format": "^1.4.1",
    "d3-scale": "^3.1.0",
    "d3-selection": "^1.4.0",
    "d3-selection-multi": "^1.0.1",
    "date-fns": "^2.5.1",
    "date-fns-tz": "^1.0.8",
    "esm": "^3.2.25",
    "file-saver": "^2.0.2",
    "jsdom": "^15.2.0",
    "mapbox-gl": "^1.4.1",
    "module-alias": "^2.2.2",
    "mustache": "^3.1.0",
    "nodemailer": "^6.3.1",
    "openid-client": "^2.4.5",
    "pdfmake": "^0.1.60",
    "pg-promise": "^9.3.3",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^3.3.3",
    "v-calendar": "^0.9.7",
    "vue": "^2.6.3",
    "vue-router": "^3.1.3",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@vue/cli-plugin-babel": "^3.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^3.12.1",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "documentation": "^12.1.2",
    "eslint": "^5.16.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-vue": "^5.1.0",
    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^9.4.2",
    "npm-run-all": "^4.1.5",
    "postcss-nested": "^4.1.1",
    "stylelint": "^11.1.1",
    "stylelint-config-standard": "^19.0.0",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.3",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "gitHooks": {
    "pre-commit": "npm-run-all pre-commit:*"
  },
  "lint-staged": {
    "lib/**/*.js": [
      "documentation lint"
    ],
    "*.js": [
      "vue-cli-service lint"
    ],
    "*.postcss": [
      "stylelint"
    ],
    "*.py": [
      "pylint"
    ],
    "*.sh": [
      "shellcheck --shell=bash"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "stylelint"
    ]
  },
  "_moduleAliases": {
    "@": "."
  },
  "browserslist": [
    "last 1 Firefox version",
    "last 1 Chrome version",
    "last 1 Edge version"
  ]
}
