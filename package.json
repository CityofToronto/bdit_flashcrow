{
  "name": "bdit_flashcrow",
  "version": "1.0.2",
  "private": true,
  "description": "An integrated data platform for City of Toronto transportation data",
  "scripts": {
    "backend": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register web/web.js",
    "backend:inspect": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect=0.0.0.0:9100 web/web.js",
    "backend:inspect-brk": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect-brk=0.0.0.0:9100 web/web.js",
    "backend:test-api": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass API_TEST_HEADLESS=1 node  -r ./globalWindowShim -r esm -r module-alias/register web/web.js",
    "ci:jest-coverage": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass jest --coverage --runInBand",
    "ci:npm-audit": "check-audit",
    "ci:npm-outdated": "ncu",
    "docs": "documentation serve --favicon public/favicon.ico --port 9000 --sort-order alpha --watch web/web.js lib/**",
    "docs:build": "documentation build web/web.js lib/** --favicon public/favicon.ico --sort-order alpha -f html -o dist/docs",
    "frontend": "vue-cli-service serve",
    "frontend:build": "vue-cli-service build",
    "pre-commit:lint-staged": "lint-staged",
    "pre-commit:test-unit-staged": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"unit/**\" jest --changedSince=master --runInBand",
    "reporter": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register reporter/reporter.js --port 8200",
    "reporter:inspect": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect=0.0.0.0:9200 reporter/reporter.js --port 8200",
    "reporter:inspect-brk": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect-brk=0.0.0.0:9200 reporter/reporter.js --port 8200",
    "reporter:test-api": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass node -r ./globalWindowShim -r esm -r module-alias/register reporter/reporter.js --port 8200",
    "scheduler": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register scheduler/scheduler.js --port 8300",
    "scheduler:inspect": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect=0.0.0.0:9300 scheduler/scheduler.js --port 8300",
    "scheduler:inspect-brk": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer node -r ./globalWindowShim -r esm -r module-alias/register --inspect-brk=0.0.0.0:9300 scheduler/scheduler.js --port 8300",
    "scheduler:test-api": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass node -r ./globalWindowShim -r esm -r module-alias/register scheduler/scheduler.js --port 8300",
    "test:db-shutdown": "./scripts/test/db/shutdown.sh",
    "test:db-startup": "./scripts/test/db/startup.sh --withDevData",
    "test:test-api": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"api/**\" jest --runInBand",
    "test:test-db": "NODE_EXTRA_CA_CERTS=./ssl/extra-ca-certs.cer NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"db/**\" jest --runInBand",
    "test:test-unit": "NODE_ENV=test PGPASSFILE=/mnt/ramdisk_move_test/.pgpass TEST_DIR=\"unit/**\" jest --runInBand"
  },
